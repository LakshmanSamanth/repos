@model List<HarshavardhanEntityFrameworkDB.Models.Brand>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPageDB.cshtml";
}

<h2>Brands</h2>

<table class="table">
    <tr>
        <th>Brand Id</th>
        <th>Brand Name</th>
    </tr>
    @foreach(var item in Model)
        {
    <tr>
        <td>@item.BrandID</td>
        <td>@item.BrandName</td>
    </tr>
      
    }
</table>

